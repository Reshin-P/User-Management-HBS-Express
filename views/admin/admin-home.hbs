      <div class="container">
        <table class="table" id="adminTable">
  <thead>
    <tr>
   
      <th scope="col">User Name</th>
      <th scope="col">Email Id</th>
      {{!-- <th scope="col">Permission</th> --}}
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>

    {{#each users}}
    <tr>
     
     
      <td>{{this.Firstname}}</td>
      <td>{{this.Email}}</td>
      {{!-- <td>@mdo</td> --}}

      <td>

     <a href="/adminactions/edituser/{{this._id}} "  onclick="return confirm(click ok to edit {{this.Firstname}})"><button  type="button" class="btn btn-info">Edit</button></a>
     {{#if this.status}}
        <a href="/adminactions/unblock/{{this._id}}" onclick="return confirm('clik ok to confirm unblock {{  this.Firstname}}')"  type="submit"> <button type="button" class="btn btn-warning">Unblock</button></a>
     
      {{else}}
      <a href="/adminactions/block/{{this._id}}" onclick="return confirm('clik ok to confirm block {{  this.Firstname}}')"  type="submit"> <button type="button" class="btn btn-warning"> Block </button></a>
     
      {{/if}}
     <a href="/adminactions/deleteuser/{{this._id}}" onclick="return confirm('clik ok to confirm delete {{  this.Firstname}}')"  type="submit"> <button type="button" class="btn btn-danger">Delete</button></a>
        



      </td>

    </tr>

    {{/each}}
   
  </tbody>
</table>
